  "bugs": {
    "url": "https://github.com/davidparsson/junit-report-builder/issues"
  },
  "homepage": "https://github.com/davidparsson/junit-report-builder",
  "engines": {
    "node": ">=8"
  },
  "devDependencies": {
    "grunt": "^1.6.1",
    "grunt-contrib-jshint": "^3.2.0",
    "grunt-contrib-watch": "^1.0.0",
    "grunt-jasmine-nodejs": "^1.6.1",
    "jit-grunt": "^0.10.0",
    "prettier": "^3.0.3",
    "release-it": "^16.2.1",
    "rimraf": "^2.3.3",
    "time-grunt": "^1.2.1"
  },
  "dependencies": {
    "date-format": "4.0.3",
